    "crash_dir": "./crashes",
    "metrics_file": "./metrics.json",
    "gene_bank_file": "./gene_bank.json",
    "enable_advanced_features": false,
    "enable_ml": false,
    "enable_analytics": false,
    "enable_security_sandbox": false,
    "sandbox_isolation_level": "none",
