{
  "local": {
    "max_generations": 10000,
    "population_size": 500,
    "mutation_rate": 0.5,
    "crossover_rate": 0.8,
    "elite_size": 20,
    "stagnation_limit": 100,
    "enable_rl": false,
    "enable_web_interface": true,
    "web_port": 8080,
    "log_level": "WARNING",
